@model RollerTest.WebUI.Models.TestListViewModel

@{
    ViewBag.Title = "Index";
    int i = 1;
}

@foreach (var p in Model.rollersampleinfos)
{
    <div class="row  border-bottom white-bg dashboard-header">

        <div class="col-sm-3">
            <h2>
            <span class="pull-right"><i id="teststate-@p.RollerBaseStation.Mark"></i></span>
            <span>@p.SampleID</span>
            </h2>
            <small>@p.RollerProjectInfo.TestStandard</small>
            <ul class="list-group clear-list m-t">
                <li class="list-group-item fist-item">
                    <span class="pull-right">
                        @p.TestType
                    </span>
                    <span class="label label-success">1</span>试验类型
                </li>
                <li class="list-group-item">
                    <span class="pull-right">
                        @p.RollerBaseStation.Station
                    </span>
                    <span class="label label-info">2</span> 工位
                </li>
                <li class="list-group-item">
                    <span class="pull-right">
                        @p.UpLimit
                    </span>
                    <span class="label label-primary">3</span> 上限值
                </li>
                <li class="list-group-item">
                    <span class="pull-right">
                        @p.DnLimit
                    </span>
                    <span class="label label-default">4</span> 下限值
                </li>
                <li class="list-group-item">
                    <span class="pull-right">
                        @p.SetValue
                    </span>
                    <span class="label label-primary">5</span> 设定值
                </li>
                @{
                    if (p.TestType == "定时截尾试验")
                    {
                        <li class="list-group-item">
                            <span class="pull-right">
                                @p.TestTime
                            </span>
                            <span class="label label-primary">6</span> 试验时间
                        </li>
                    }
                }
            </ul>
        </div>
        <div class="col-sm-6">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>应力曲线图</h5>
                </div>
                <div class="ibox-content">
                    <div class="flot-chart">
                        <div class="flot-chart-content" id="flot-moving-@p.RollerBaseStation.Mark"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-3">
                <div class="tabs-container">
                    <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" href="#tab-A-@i"> <i class="fa fa-laptop">数据</i></a></li>
                        <li class=""><a data-toggle="tab" href="#tab-B-@i"><i class="fa fa-exchange">过程</i></a></li>
                        <li class=""><a data-toggle="tab" href="#tab-C-@i"><i class="fa fa-ban">故障</i></a></li>
                    </ul>
                    <div class="tab-content">
                        <div id="tab-A-@i" class="tab-pane active">
                            <div class="panel-body">
                                <div class="ibox-content">
                                    <div>
                                        <div class="feed-activity-list">
                                            <div class="feed-element">
                                                <div class="media-body">
                                                    <small>Time</small>
                                                    <h1 id="time-@p.RollerBaseStation.Mark" class="no-margins"></h1>
                                                    <div class="pull-right">
                                                        <div>
                                                            <button class="btn btn-xs btn-white" id="button-@p.RollerBaseStation.Mark">开启</button>
                                                            <button class="btn btn-xs btn-white" id="pbutton-@p.RollerBaseStation.Mark">暂停</button>
                                                            <button class="btn btn-xs btn-white" id="cbutton-@p.RollerBaseStation.Mark">清零</button>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="feed-element">
                                                <div class="media-body">
                                                    <small>Force</small>
                                                    <h1 id="data-@p.RollerBaseStation.Mark" class="no-margins"></h1>
                                                    <div class="pull-right">
                                                        <div>
                                                            <button class="btn btn-xs btn-white" id="ofbutton-@p.RollerBaseStation.Mark">开启</button>
                                                            <button class="btn btn-xs btn-white" id="cfbutton-@p.RollerBaseStation.Mark">关闭</button>
                                                        </div>
                                                    </div>
                                                    </div>
                                                </div>
                                        </div>

                                    </div>
                                </div>
                                </div>
                        </div>
                        <div id="tab-B-@i" class="tab-pane">
                            <div class="panel-body">
                                <ul class="list-group clear-list m-t">
                                    <li class="list-group-item fist-item">
                                        <span class="pull-right">
                                            <button class="btn btn-xs btn-white" id="otbutton-@p.RollerBaseStation.Mark">开启</button>
                                            @Html.ActionLink("记录", "Index", new { Controller = "TestReport", RollerSampleInfoId = p.RollerSampleInfoID }, new { @class = "btn btn-xs btn-white" })
                                        </span>
                                        <span class="label label-success">1</span>开启试验
                                    </li>
                                    <li class="list-group-item">
                                        <span class="pull-right">
                                            <button class="btn btn-xs btn-white" id="ptbutton-@p.RollerBaseStation.Mark">暂停</button>
                                            @Html.ActionLink("记录", "Index", new { Controller="SampleTestingRecord", RollerSampleInfoId = p.RollerSampleInfoID }, new { @class = "btn btn-xs btn-white" })
                                        </span>
                                        <span class="label label-info">2</span> 中间过程
                                    </li>
                                    <li class="list-group-item">
                                        <span class="pull-right">
                                            <button class="btn btn-xs btn-white" id="ctbutton-@p.RollerBaseStation.Mark">关闭</button>
                                            @Html.ActionLink("记录", "AfterTest", new { Controller = "TestReport", RollerSampleInfoId = p.RollerSampleInfoID }, new { @class = "btn btn-xs btn-white" })
                                        </span>
                                        <span class="label label-primary">3</span> 结束试验
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <div id="tab-C-@i" class="tab-pane">
                            <div class="panel-body">
                                <div class="ibox-content">
                                    <div>
                                        <div class="feed-activity-list">
                                            <div class="feed-element">
                                                <div class="media-body">
                                                    <b>寿命时间</b><br />
                                                    @{
                                                        if (p.RollerRecordInfo.Count() != 0)
                                                        {
                                                            foreach (var x in @p.RollerRecordInfo.Where(x => x.SampleStatus == false))
                                                            {
                                                                <p>@x.TotalTime</p>
                                                            }
                                                        }
                                                    }
                                                </div>
                                            </div>
                                            <div class="feed-element">
                                                <div class="media-body">
                                                    <b>故障记录</b><br/>
                                                    @{
                                                        if (p.RollerRecordInfo.Count() != 0)
                                                        {
                                                            foreach(var x in @p.RollerRecordInfo.Where(x => x.SampleStatus == false))
                                                            {
                                                                <p>@x.RecordInfo</p>
                                                            }
                                                        }
                                                    }
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                        </div>
                    </div>
                </div>
        </div>

    </div>
    i++;
}
