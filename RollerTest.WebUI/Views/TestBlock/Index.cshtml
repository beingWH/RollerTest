                        case "开始": strn = "fa fa-play"; break;
                        case "暂停": strn = "fa fa-pause"; break;
                        case "结束": strn = "fa fa-stop"; break;
                        case "故障": strn = "fa fa-warning"; break;
                        case "待结束": strn = "fa fa-coffee"; break;
                        default: strn = "fa fa-apple"; break;
